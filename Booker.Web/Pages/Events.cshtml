@page
@model Booker.Web.Pages.EventsModel
@{
    ViewData["Title"] = "Andale Events";
}


<h1 class="display-4">Events</h1>

<p class="lead">Here you see a table with all events available</p>
<hr />

<table class="table table-striped table-bordered dt-responsive flex-nowrap  text-center">
    <thead class="thead-dark">
        <tr>
            <th>Event Id</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var eventItem in Model.Events)
        {
            <tr>
                <td>@eventItem.Id</td>
                <td>@eventItem.Description</td>
            </tr>
        }

    </tbody>
</table>
